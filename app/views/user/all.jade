extends ../layouts/default

block main
	h1.page-header Players
	each profile in profiles
		.row
			.col-lg-1
				img(src=profile.google.picture || "https://ssl.gstatic.com/s2/profiles/images/silhouette120.png", height="27", width="27")				
			.col-lg-4
				a(href="/user/profile/" + profile.id) #{profile.name}

